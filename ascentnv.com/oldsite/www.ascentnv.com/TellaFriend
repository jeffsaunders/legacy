<SCRIPT language="javascript">

	//This function needs to be called from functions.js
	// Uncommented 8/5/03 By Dan - functions.js imports don't seem to be working, need to investigate.  For now local will do.
	function validEmail(szEmail) {
		var nIndexAT = szEmail.indexOf("@",1);
		var nIndexDot = szEmail.indexOf(".", nIndexAT);
		var nCounter, nIndexBad;
		var szInvalid = "/,;"

		if(szEmail != null || szEmail != "") {
			if (nIndexAT < 0 || nIndexDot < 0) {
				return false;
			} 
			for(nCounter=0; nCounter < szInvalid.length; nCounter++) {
				if (szEmail.indexOf(szInvalid.charAt(nCounter)) >= 0) {
					return false;
					break;
				}
			}
		}
		return true
	}
	

	function formCheck() {
		//if (!validEmail(document.frmFriend.txtFrom.value)) {
		//	alert('The from email address does not appear to be valid.  Please check the email address and try again.');
		//	return false;
		//}
		if (!validEmail(document.frmFriend.txtRecp1.value)) {
			alert('The email address for recipient 1 does not appear to be valid.  Please check the email address and try again.');
			return false;
		}
		if (document.frmFriend.txtRecp2.value != "" && document.frmFriend.txtRecp2.value != null) {
			if (!validEmail(document.frmFriend.txtRecp2.value)) {
				alert('The email address for recipient 2 does not appear to be valid.  Please check the email address and try again.');
				return false;
			}
		}
		if (document.frmFriend.txtRecp3.value != "" && document.frmFriend.txtRecp3.value != null) {
			if (!validEmail(document.frmFriend.txtRecp3.value)) {
				alert('The email address for recipient 3 does not appear to be valid.  Please check the email address and try again.');
				return false;
			}
		}
		document.frmFriend.submit();
	}		
</SCRIPT>

